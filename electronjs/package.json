{
  "name": "azure-role-activater",
  "version": "1.0.0",
  "description": "Used to activate your assigned Priviledged Role Management roles and add your ip address to azure keyvault and storage accounts",
  "main": "main.js",
  "scripts": {
    "prestart": "tsc",
    "start": "conc -k \"npm run dev  --prefix ..\\react-ui\"  \"wait-on http://localhost:3000 && electron build/main.js\" ",
    "start2": "tsc && electron build/main.js & npm run dev --prefix ..\\react-ui",
    "electron-run": "electron build/main.js",
    "eslint": "tsc && npx eslint build/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naveenrenold/azure-role-activator.git"
  },
  "keywords": [
    "PIM",
    "Priviledged",
    "Role",
    "ip",
    "ipupdate",
    "Azure",
    "AutoUpdateRole"
  ],
  "author": "Naveen Renold",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/naveenrenold/azure-role-activator/issues"
  },
  "homepage": "https://github.com/naveenrenold/azure-role-activator#readme",
  "devDependencies": {
    "@eslint/compat": "^1.0.1",
    "@eslint/js": "^9.3.0",
    "@types/node": "^20.12.12",
    "electron": "^30.0.4",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.2.0",
    "typescript-eslint": "^7.9.0"
  },
  "dependencies": {
    "@azure/msal-node": "^2.8.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "concurrently": "^8.2.2",
    "typescript": "^5.4.5",
    "wait-on": "^7.2.0"
  }
}
